{
  "base": {
    "numShards": 48,
    "opsThreshold": 500000000,
    "ageThresholdMs": 86400000,
    "reencryptionEnabled": true,
    "forwardSecurityEnabled": true,
    "partitionedIndexingEnabled": true,
    "profilerEnabled": true,

    "paper": {
      "enabled": true,
      "m": 24,
      "lambda": 2,
      "divisions": 3,
      "seed": 13,
      "safetyMaxCandidates": 0,
      "probeLimit": 6
    },

    "stabilization": {
      "enabled": true,
      "alpha": 0.15,
      "minCandidates": 2000
    },

    "eval": {
      "computePrecision": true,
      "writeGlobalPrecisionCsv": true,
      "kVariants": [1, 5, 10, 20, 50, 100]
    },

    "ratio": {
      "source": "gt",
      "gtSample": 10000,
      "autoComputeGT": false
    },

    "output": {
      "exportArtifacts": true,
      "suppressLegacyMetrics": true
    }

},

"profiles": [

