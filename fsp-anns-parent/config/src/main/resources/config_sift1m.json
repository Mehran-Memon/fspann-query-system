{
  "base": {
    "opsThreshold": 500000000,
    "ageThresholdMs": 86400000,
    "reencryptionEnabled": true,
    "forwardSecurityEnabled": true,
    "profilerEnabled": true,

    "paper": {
      "enabled": true,
      "m": 24,
      "lambda": 2,
      "divisions": 3,
      "tables": 4,
      "seed": 13
    },

    "stabilization": {
      "enabled": true,
      "alpha": 0.02,
      "minCandidatesRatio": 1.25
    },

    "runtime": {
      "maxCandidateFactor": 3,
      "maxRefinementFactor": 3,
      "maxRelaxationDepth": 1,
      "earlyStopCandidates": 1000
    },

    "eval": {
      "computePrecision": true,
      "writeGlobalPrecisionCsv": true,
      "kVariants": [1, 5, 10, 20, 50, 100]
    },

    "ratio": {
      "source": "gt",
      "gtSample": 100
    }
  },

  "profiles": [
    { "name": "M18", "overrides": { "paper": { "m": 18 } } },
    { "name": "M20", "overrides": { "paper": { "m": 20 } } },
    { "name": "M24", "overrides": { "paper": { "m": 24 } } },
    { "name": "M28", "overrides": { "paper": { "m": 28 } } },
    { "name": "M32", "overrides": { "paper": { "m": 32 } } },

    { "name": "L1", "overrides": { "paper": { "lambda": 1 } } },
    { "name": "L2", "overrides": { "paper": { "lambda": 2 } } },
    { "name": "L3", "overrides": { "paper": { "lambda": 3 } } },

    { "name": "D3", "overrides": { "paper": { "divisions": 3, "tables": 4 } } },
    { "name": "D4", "overrides": { "paper": { "divisions": 4, "tables": 4 } } },
    { "name": "D5", "overrides": { "paper": { "divisions": 5, "tables": 4 } } }
  ]
}
