{
  "opsThreshold": 500000000,
  "ageThresholdMs": 86400000,
  "reencryptionEnabled": true,
  "forwardSecurityEnabled": true,
  "profilerEnabled": true,

  "paper": {
    "enabled": true,
    "m": 24,
    "lambda": 2,
    "divisions": 8,
    "tables": 5,
    "seed": 13
  },

  "stabilization": {
    "enabled": false,
    "alpha": 0.02,
    "minCandidatesRatio": 1.20
  },

  "runtime": {
    "precisionMode": false,
    "refinementLimit": 2500,
    "maxCandidateFactor": 3,
    "maxRelaxationDepth": 4
  },

  "eval": {
    "computePrecision": true,
    "writeGlobalPrecisionCsv": true,
    "kVariants": [1, 20, 40, 60, 80, 100]
  },

  "ratio": {
    "source": "gt",
    "gtSample": 200,
    "gtMismatchTolerance": 0.05
  },

"profiles": [
    { "name": "M14", "overrides": { "paper": { "m": 14, "lambda": 2, "divisions": 8, "tables": 5 } } },
    { "name": "M18", "overrides": { "paper": { "m": 18, "lambda": 2, "divisions": 8, "tables": 5 } } },
    { "name": "M20", "overrides": { "paper": { "m": 20, "lambda": 2, "divisions": 8, "tables": 5 } } },
    { "name": "M24", "overrides": { "paper": { "m": 24, "lambda": 2, "divisions": 8, "tables": 5 } } },
    { "name": "M28", "overrides": { "paper": { "m": 28, "lambda": 2, "divisions": 8, "tables": 5 } } },
    { "name": "M32", "overrides": { "paper": { "m": 32, "lambda": 2, "divisions": 8, "tables": 5 } } },

    { "name": "L3", "overrides": { "paper": { "m": 24, "lambda": 3, "divisions": 8, "tables": 8 } } },

    { "name": "D2", "overrides": { "paper": { "m": 20, "lambda": 2, "divisions": 2, "tables": 5 } } }
  ]
