{
  "base": {
    "numShards": 32,
    "opsThreshold": 500000000,
    "ageThresholdMs": 86400000,
    "reencryptionEnabled": true,
    "forwardSecurityEnabled": true,
    "partitionedIndexingEnabled": true,
    "profilerEnabled": true,

    "paper": {
      "enabled": true,
      "m": 24,
      "lambda": 2,
      "divisions": 3,
      "seed": 13,
      "safetyMaxCandidates": 0
    },

    "lsh": { "numTables": 0 },

    "stabilization": {
      "enabled": true,
      "alpha": 0.10,
      "minCandidates": 1200
    },

    "reencryption": {
      "enabled": true,
      "batchSize": 1024,
      "maxMsPerBatch": 50
    },

    "eval": {
      "computePrecision": true,
      "writeGlobalPrecisionCsv": true,
      "kVariants": [1, 10, 20, 50, 100]
    },

    "output": {
      "resultsDir": "results",
      "exportArtifacts": true,
      "suppressLegacyMetrics": true
    },

    "ratio": {
      "source": "gt",
      "gtPath": null,
      "gtSample": 10000,
      "autoComputeGT": false
    }
  },

  "profiles": [

    { "name": "M12", "overrides": { "paper": { "m": 12 } } },
    { "name": "M18", "overrides": { "paper": { "m": 18 } } },
    { "name": "M24", "overrides": { "paper": { "m": 24 } } },
    { "name": "M30", "overrides": { "paper": { "m": 30 } } },

    { "name": "L1", "overrides": { "paper": { "lambda": 1 } } },
    { "name": "L2", "overrides": { "paper": { "lambda": 2 } } },
    { "name": "L3", "overrides": { "paper": { "lambda": 3 } } },
    { "name": "L4", "overrides": { "paper": { "lambda": 4 } } },

    { "name": "D2", "overrides": { "paper": { "divisions": 2 } } },
    { "name": "D3", "overrides": { "paper": { "divisions": 3 } } },
    { "name": "D4", "overrides": { "paper": { "divisions": 4 } } },

    { "name": "A0.05", "overrides": { "stabilization": { "alpha": 0.05 } } },
    { "name": "A0.10", "overrides": { "stabilization": { "alpha": 0.10 } } },
    { "name": "A0.15", "overrides": { "stabilization": { "alpha": 0.15 } } }
  ]
}
