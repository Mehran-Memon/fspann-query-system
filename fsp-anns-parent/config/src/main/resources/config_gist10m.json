{
  "opsThreshold": 500000000,
  "ageThresholdMs": 86400000,
  "reencryptionEnabled": true,
  "forwardSecurityEnabled": true,
  "profilerEnabled": true,

  "paper": {
    "enabled": true,
    "m": 28,
    "lambda": 2,
    "divisions": 4,
    "tables": 4,
    "seed": 13
  },

  "stabilization": {
    "enabled": true,
    "alpha": 0.02,
    "minCandidatesRatio": 1.25
  },

  "runtime": {
    "precisionMode": true,
    "minPrecisionCandidates": 10000,
    "maxPrecisionCandidates": 40000,
    "refinementLimit": 20000,
    "maxCandidateFactor": 120,
    "maxRelaxationDepth": 3
  },

  "eval": {
    "computePrecision": true,
    "writeGlobalPrecisionCsv": true,
    "kVariants": [1, 10, 50, 100]
  },

  "ratio": {
    "source": "gt",
    "gtSample": 50
  },

  "profiles": [
    { "name": "M28", "overrides": { "paper": { "m": 28, "lambda": 2, "divisions": 4, "tables": 4 } } },
    { "name": "M32", "overrides": { "paper": { "m": 32, "lambda": 2, "divisions": 4, "tables": 4 } } },

    { "name": "L2", "overrides": { "paper": { "m": 28, "lambda": 2, "divisions": 4, "tables": 4 } } }
  ]
}
