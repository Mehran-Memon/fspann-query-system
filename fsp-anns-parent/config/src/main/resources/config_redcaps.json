{
  "opsThreshold": 500000000,
  "ageThresholdMs": 86400000,
  "reencryptionEnabled": true,
  "forwardSecurityEnabled": true,
  "profilerEnabled": true,

  "paper": {
    "enabled": true,
    "m": 24,
    "lambda": 2,
    "divisions": 3,
    "tables": 4,
    "seed": 13
  },

  "stabilization": {
    "enabled": true,
    "alpha": 0.02,
    "minCandidatesRatio": 1.25
  },

  "runtime": {
    "precisionMode": true,
    "minPrecisionCandidates": 8000,
    "maxPrecisionCandidates": 30000,
    "refinementLimit": 15000,
    "maxCandidateFactor": 200,
    "maxRelaxationDepth": 4
  },

  "eval": {
    "computePrecision": true,
    "writeGlobalPrecisionCsv": true,
    "kVariants": [1, 5, 10, 20, 50, 100]
  },

  "ratio": {
    "source": "gt",
    "gtSample": 100
  },

  "profiles": [
    { "name": "M20", "overrides": { "paper": { "m": 20, "lambda": 2, "divisions": 3, "tables": 4 } } },
    { "name": "M24", "overrides": { "paper": { "m": 24, "lambda": 2, "divisions": 3, "tables": 4 } } },
    { "name": "M28", "overrides": { "paper": { "m": 28, "lambda": 2, "divisions": 3, "tables": 4 } } },
    { "name": "M32", "overrides": { "paper": { "m": 32, "lambda": 2, "divisions": 3, "tables": 4 } } },

    { "name": "L2", "overrides": { "paper": { "m": 24, "lambda": 2, "divisions": 3, "tables": 4 } } },
    { "name": "L3", "overrides": { "paper": { "m": 24, "lambda": 3, "divisions": 3, "tables": 4 } } },

    { "name": "D2", "overrides": { "paper": { "m": 24, "lambda": 2, "divisions": 2, "tables": 4 } } },
    { "name": "D3", "overrides": { "paper": { "m": 24, "lambda": 2, "divisions": 3, "tables": 4 } } },
    { "name": "D4", "overrides": { "paper": { "m": 24, "lambda": 2, "divisions": 4, "tables": 4 } } }
  ]
}
