{
  "opsThreshold": 500000000,
  "ageThresholdMs": 86400000,
  "reencryptionEnabled": true,
  "forwardSecurityEnabled": true,
  "profilerEnabled": true,

  "paper": {
    "enabled": true,
    "m": 24,
    "lambda": 2,
    "divisions": 4,
    "tables": 8,
    "seed": 13
  },

  "stabilization": {
    "enabled": true,
    "alpha": 0.015,
    "minCandidatesRatio": 1.25
  },

  "runtime": {
    "precisionMode": false,
    "refinementLimit": 3500,
    "maxCandidateFactor": 4,
    "maxRelaxationDepth": 6
  },

  "eval": {
    "computePrecision": true,
    "writeGlobalPrecisionCsv": true,
    "kVariants": [1, 20, 40, 60, 80, 100]
  },

  "ratio": {
    "source": "gt",
    "gtSample": 200,
    "gtMismatchTolerance": 0.05
  },

  "profiles": [
    { "name": "M20", "overrides": { "paper": { "m": 20, "lambda": 2, "divisions": 4, "tables": 8 } } },
    { "name": "M24", "overrides": { "paper": { "m": 24, "lambda": 2, "divisions": 4, "tables": 8 } } },
    { "name": "M28", "overrides": { "paper": { "m": 28, "lambda": 2, "divisions": 4, "tables": 8 } } },

    { "name": "L2", "overrides": { "paper": { "m": 24, "lambda": 2, "divisions": 4, "tables": 8 } } },
    { "name": "L3", "overrides": { "paper": { "m": 24, "lambda": 3, "divisions": 4, "tables": 8 } } },

    { "name": "D2", "overrides": { "paper": { "m": 24, "lambda": 2, "divisions": 2, "tables": 8 } } },
    { "name": "D3", "overrides": { "paper": { "m": 24, "lambda": 2, "divisions": 3, "tables": 8 } } },
    { "name": "D5", "overrides": { "paper": { "m": 24, "lambda": 2, "divisions": 5, "tables": 8 } } }

  ]
}
