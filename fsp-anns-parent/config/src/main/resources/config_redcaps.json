{
  "base": {
    "numShards": 48,
    "opsThreshold": 500000000,
    "ageThresholdMs": 86400000,
    "reencryptionEnabled": true,
    "forwardSecurityEnabled": true,
    "partitionedIndexingEnabled": true,
    "profilerEnabled": true,

    "paper": {
      "enabled": true,
      "m": 32,
      "lambda": 2,
      "divisions": 4,
      "seed": 13,
      "safetyMaxCandidates": 0,
      "probeLimit": 6
    },

    "stabilization": {
      "enabled": true,
      "alpha": 0.15,
      "minCandidates": 2000
    },

    "eval": {
      "computePrecision": true,
      "writeGlobalPrecisionCsv": true,
      "kVariants": [1, 5, 10, 20, 50, 100]
    },

    "ratio": {
      "source": "gt",
      "gtSample": 10000,
      "autoComputeGT": false
    },

    "output": {
      "exportArtifacts": true,
      "suppressLegacyMetrics": true
    }

},

  "profiles": [

    { "name": "M10", "overrides": { "paper": { "m": 10, "lambda": 3, "divisions": 3, "seed": 13, "safetyMaxCandidates": 0 } } },
    { "name": "M15", "overrides": { "paper": { "m": 15, "lambda": 3, "divisions": 3, "seed": 13, "safetyMaxCandidates": 0 } } },
    { "name": "M20", "overrides": { "paper": { "m": 20, "lambda": 3, "divisions": 3, "seed": 13, "safetyMaxCandidates": 0 } } },
    { "name": "M25", "overrides": { "paper": { "m": 25, "lambda": 3, "divisions": 3, "seed": 13, "safetyMaxCandidates": 0 } } },
    { "name": "M30", "overrides": { "paper": { "m": 30, "lambda": 3, "divisions": 3, "seed": 13, "safetyMaxCandidates": 0 } } },

    { "name": "L1", "overrides": { "paper": { "m": 25, "lambda": 1, "divisions": 3, "seed": 13, "safetyMaxCandidates": 0 } } },
    { "name": "L2", "overrides": { "paper": { "m": 25, "lambda": 2, "divisions": 3, "seed": 13, "safetyMaxCandidates": 0 } } },
    { "name": "L3", "overrides": { "paper": { "m": 25, "lambda": 3, "divisions": 3, "seed": 13, "safetyMaxCandidates": 0 } } },

    { "name": "D2", "overrides": { "paper": { "m": 25, "lambda": 3, "divisions": 2, "seed": 13, "safetyMaxCandidates": 0 } } },
    { "name": "D3", "overrides": { "paper": { "m": 25, "lambda": 3, "divisions": 3, "seed": 13, "safetyMaxCandidates": 0 } } },
    { "name": "D4", "overrides": { "paper": { "m": 25, "lambda": 2, "divisions": 4, "seed": 13, "safetyMaxCandidates": 0 } } },

    { "name": "A0.05", "overrides": { "stabilization": { "alpha": 0.05 } } },
    { "name": "A0.10", "overrides": { "stabilization": { "alpha": 0.10 } } },
    { "name": "A0.15", "overrides": { "stabilization": { "alpha": 0.15 } } }
  ]
}
