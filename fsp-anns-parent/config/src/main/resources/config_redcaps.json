{
  "base": {
    "numShards": 24,
    "opsThreshold": 500000000,
    "reencryptionEnabled": true,
    "forwardSecurityEnabled": true,
    "partitionedIndexingEnabled": true,

    "paper": {
      "enabled": true,
      "m": 20,
      "lambda": 3,
      "divisions": 3,
      "seed": 13,
      "safetyMaxCandidates": 0
    },

    "stabilization": {
      "enabled": true,
      "alpha": 0.12,
      "minCandidates": 1400
    },

    "eval": {
      "computePrecision": true,
      "writeGlobalPrecisionCsv": true,
      "kVariants": [1, 5, 10, 20, 50, 100]
    },

    "output": {
      "exportArtifacts": true
    }
  },

